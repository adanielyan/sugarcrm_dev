	<queryString>
		<![CDATA[select c.*, a.id as account_id, a.name as account_name from
meetings_contacts mc, contacts c
LEFT OUTER JOIN accounts_contacts ac ON c.id = ac.contact_id
LEFT OUTER JOIN accounts a ON ac.account_id = a.id
where
mc.meeting_id = $P{MEETING_ID} and mc.contact_id = c.id and
c.deleted = 0 and mc.deleted = 0 order by c.last_name, c.first_name]]>
	</queryString>
