	<queryString><![CDATA[select o.name, o.date_entered, o.date_closed, o.sales_stage, o.amount, o.assigned_user_id, o.lead_source, u.first_name, u.last_name , to_days($P{date}) - to_days(o.date_entered) as open_time, o.probability
from opportunities o
inner join users u on o.assigned_user_id = u.id
where to_days($P{date}) - to_days(o.date_entered) between 0 and $P{range_days}
and sales_stage = 'Closed Lost'
order by u.id]]></queryString>
