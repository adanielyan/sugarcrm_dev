	<queryString><![CDATA[SELECT m.*, 
case m.parent_type 
when "Accounts" then a.name 
when "Opportunities" then o.name 
when "Cases" then c.name 
when "Leads" then concat(l.first_name, " ", l.last_name) 
when "Bugs" then b.name 
end as parent_name
FROM meetings m
LEFT OUTER JOIN accounts a ON m.parent_id = a.id
LEFT OUTER JOIN opportunities o ON m.parent_id = o.id
LEFT OUTER JOIN cases c ON m.parent_id = c.id
LEFT OUTER JOIN leads l ON m.parent_id = l.id
LEFT OUTER JOIN bugs b ON m.parent_id = b.id
WHERE m.id = $P{MEETING_ID}]]></queryString>
